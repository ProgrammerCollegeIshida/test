package jp.co.internous.action;

public class Instantiation {

	public static void main(String[] args) {

Class instance0 = new Class();
instance0.name = "Simon Bird";
instance0.age = 35;
System.out.println(instance0.name);
System.out.println(instance0.age);

Class instance1 = new Class();
instance1.name = "Joe Thomas";
instance1.age = 35;

	}

}
